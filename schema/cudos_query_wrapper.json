{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CudosQueryWrapper",
  "description": "CudosQueryWrapper is an override of QueryRequest::Custom to access Cudos-specific modules",
  "type": "object",
  "required": [
    "query_data"
  ],
  "properties": {
    "query_data": {
      "$ref": "#/definitions/CudosQuery"
    }
  },
  "definitions": {
    "CudosQuery": {
      "description": "CudosQuery is defines available query datas",
      "oneOf": [
        {
          "type": "object",
          "required": [
            "denom"
          ],
          "properties": {
            "denom": {
              "type": "object",
              "required": [
                "denom_id"
              ],
              "properties": {
                "denom_id": {
                  "type": "string"
                }
              }
            }
          },
          "additionalProperties": false
        }
      ]
    }
  }
}
